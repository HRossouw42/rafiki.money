{
  "name": "agreements-service",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=10.13"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src/**/*.json",
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "build/doc"
  ],
  "scripts": {
    "start": "npm run build && node ./build/src/start.js",
    "clean": "rm -Rf .nyc_output && rm -Rf coverage && rm -Rf build ",
    "build": "tsc && cp src/services/take.lua src/services/agreement-balance.lua ./build/src/services/",
    "lint": "standardx \"./src/**/*.ts\"",
    "lint:fix": "standardx --fix \"./src/**/*.ts\"",
    "make:migration": "knex migrate:make ",
    "make:seed": "knex seed:make ",
    "migrate:latest": "knex migrate:latest",
    "seed": "knex seed:run",
    "test": "jest --bail --runInBand",
    "preversion": "npm run lint && npm test",
    "prepublishOnly": "rm -Rf ./build/test && npm run lint && npm test && npm run doc"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/ioredis": "^4.0.13",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cors": "0.0.0",
    "@types/koa-joi-router": "^5.2.2",
    "@types/uuid": "^3.4.4",
    "ilp-packet": "^3.0.8",
    "ilp-plugin-btp": "^1.3.11",
    "ilp-protocol-stream": "^1.8.9",
    "ioredis": "^4.14.0",
    "iso8601-duration": "^1.2.0",
    "knex": "^0.20.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "0.0.16",
    "koa-joi-router": "^6.0.0",
    "mysql": "^2.17.1",
    "objection": "^1.6.9",
    "sqlite3": "^4.1.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/get-port": "^4.2.0",
    "@types/jest": "^24.0.19",
    "@types/sqlite3": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "axios": "^0.19.0",
    "eslint-plugin-import": "^2.18.2",
    "ioredis-mock": "^4.16.3",
    "jest": "^24.9.0",
    "mockttp": "^0.15.0",
    "standardx": "^5.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.2"
  }
